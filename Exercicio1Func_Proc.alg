algoritmo "Exercicio1Funcao_Proced"
var
   x: inteiro

//inicio função
funcao dobro (n1: inteiro):inteiro
inicio
retorne n1*2
fimfuncao


//inicio algoritmo
inicio
      escreva("Digite um numero: ")
      leia(x)
      
      escreval("O dobro do numero", x, " é: ", dobro(x))
      
      
fimalgoritmo
